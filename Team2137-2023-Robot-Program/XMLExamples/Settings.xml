                        <V>2.2607</V>
                        <A>0.15911</A>
                    </PID>
                </Motor>

                <Encoder>
                    <Name>Turn Encoder</Name>
                    <ID>22</ID>
                    <Type>CTRE_CAN_ABS</Type>
                    <Inverted>false</Inverted>
                    <Offset>-152.6001</Offset>
                </Encoder>
            </SwerveModule>

            <SwerveModule type="SIMULATION" name="RightFront">
                <Motor>
                    <Note>This is the Right Front Motor on the Swerve Module</Note>
                    <Name>Turn Motor</Name>
                    <ID>16</ID>
                    <Type>FALCON</Type>
                    <Inverted>false</Inverted>
                    <GearRatio>1</GearRatio>
