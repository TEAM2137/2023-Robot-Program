                    <ID>21</ID>
                    <Type>FALCON</Type>
                    <Inverted>false</Inverted>
                    <GearRatio>1</GearRatio>
                    <CurrentLimit>60</CurrentLimit>
                    <RampRate>0</RampRate>
                    <PID Slot="0">
                        <P>0.2</P>
                        <I>0.0</I>
