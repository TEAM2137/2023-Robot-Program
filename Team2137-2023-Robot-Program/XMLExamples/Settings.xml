                        <I>0.0</I>
                        <D>0.1</D>
                        <FF>0.75</FF>
                    </PID>
                </Motor>

                <Motor>
                    <Name>Drive Motor</Name>
                    <ID>25</ID>
                    <Type>FALCON</Type>
                    <Inverted>false</Inverted>
                    <GearRatio>1</GearRatio>
                    <CurrentLimit>60</CurrentLimit>
                    <RampRate>0</RampRate>
                    <PID Slot="0">
                        <P>0.10237</P>
                        <I>0.0</I>
                        <D>0.0</D>

