                    <RampRate>0</RampRate>
                    <PID Slot="0">
                        <P>0.10237</P>
                        <I>0.0</I>
                        <D>0.0</D>

                        <S>0.64728</S>
                        <V>2.2607</V>
                        <A>0.15911</A>
                    </PID>
                </Motor>

                <Encoder>
                    <Name>Turn Encoder</Name>
                    <ID>17</ID>
                    <Type>CTRE_CAN_ABS</Type>
                    <Inverted>false</Inverted>
                    <Offset>-169.1894</Offset>
                </Encoder>
            </SwerveModule>

            <SwerveModule type="SIMULATION" name="RightBack">
                <Motor>
                    <Note>This is the Right Back Back Motor on the Swerve Module</Note>
                    <Name>Turn Motor</Name>
                    <ID>26</ID>
                    <Type>FALCON</Type>
                    <Inverted>false</Inverted>
                    <GearRatio>1</GearRatio>
                    <CurrentLimit>60</CurrentLimit>
                    <RampRate>0</RampRate>
