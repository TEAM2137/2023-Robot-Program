                    <Name>Turn Motor</Name>
                    <ID>11</ID>
                    <Type>FALCON</Type>
                    <Inverted>false</Inverted>
                    <GearRatio>1</GearRatio>
                    <CurrentLimit>60</CurrentLimit>
                    <RampRate>0</RampRate>
                    <PID Slot="0">
                        <P>0.2</P>
                        <I>0.0</I>
                        <D>0.1</D>
                        <FF>0.75</FF>
                    </PID>
                </Motor>

                <Motor>
